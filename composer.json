{
    "name": "brianhenryie/bh-awp-add-affiliates-to-klaviyo",
    "description": "This is a short description of what plugin_title does.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0+-or-later",
    "authors": [
        {
            "name": "Brian Henry",
            "email": "BrianHenryIE@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "repositories": [
        {
            "url": "https://github.com/WordPress/wordpress-develop",
            "type": "git"
        },
        {
            "url": "https://github.com/pablo-sg-pacheco/wp-namespace-autoloader",
            "type": "git"
        },
        {
            "url": "https://github.com/BrianHenryIE/WPPB-lib",
            "type": "git"
        },
        {
            "url": "https://github.com/BrianHenryIE/composer-phpstorm",
            "type": "git"
        },
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        },
        {
            "type": "path",
            "url": "../bh-wp-logger"
        },
        {
            "url": "https://github.com/BrianHenryIE/bh-wp-logger",
            "type": "git"
        }
    ],
    "require": {
        "php": ">=7.2",
        "brianhenryie/wp-logger": "dev-master",
        "brianhenryie/wppb-lib": "dev-master",
        "klaviyo/php-sdk": "^2.2",
        "pablo-sg-pacheco/wp-namespace-autoloader": "dev-master"
    },
    "require-dev": {
        "10up/wp_mock": "0.4.2",
        "brianhenryie/composer-phpstorm": "dev-master",
        "codeception/module-asserts": "^1.0",
        "codeception/module-cli": "^1.0",
        "codeception/module-db": "^1.0.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-webdriver": "^1.0",
        "codeception/util-universalframework": "^1.0",
        "coenjacobs/mozart": "dev-master#17268e232f4bc88b651fbcf3c6b961ffddad53fc",
        "cweagans/composer-patches": "~1.0",
        "dealerdirect/phpcodesniffer-composer-installer": "*",
        "kporras07/composer-symlinks": "dev-master",
        "lucatume/wp-browser": "*",
        "phpcompatibility/phpcompatibility-wp": "*",
        "phpunit/phpcov": "^5.0",
        "phpunit/phpunit": ">=7.0",
        "voku/portable-ascii": "dev-master",
        "wordpress/wordpress": "^5.4",
        "wp-cli/wp-cli-bundle": "^2.4",
        "wp-coding-standards/wpcs": "*",
        "wpackagist-theme/twentytwenty": "*"
    },
    "extra": {
        "mozart": {
            "dep_namespace": "BH_AWP_Add_Affiliates_to_Klaviyo\\",
            "dep_directory": "/src/vendor/",
            "classmap_prefix": "BH_AWP_Add_Affiliates_to_Klaviyo_",
            "classmap_directory": "/src/dependencies/",
            "delete_vendor_directories": false,
            "override_autoload": {
                "brianhenryie/wp-logger": {
                    "psr-4": {
                        "BrianHenryIE\\WP_Logger\\": "src"
                    }
                },
                "katzgrau/klogger": {
                    "psr-4": {
                        "Katzgrau\\KLogger\\": "src/"
                    }
                }
            }
        },
        "symlinks": {
            "wp-content": "wordpress/wp-content",
            "src": "wp-content/plugins/bh-awp-add-affiliates-to-klaviyo"
        },
        "phpstorm": {
            "exclude_folders": {
                "folders": [
                    "vendor/wordpress/wordpress/src",
                    "vendor/wordpress/wordpress/build",
                    "wp-content/plugins/bh-awp-add-affiliates-to-klaviyo",
                    "wordpress/wp-content/plugins"
                ],
                "include_folders": [
                    "vendor/wordpress/wordpress/"
                ],
                "composer-symlinks": false
            }
        }
    },
    "scripts": {
        "post-install-cmd": [
            "\"vendor/bin/mozart\" compose",
            "vendor/bin/wp core download --path=wordpress || true;",
            "export $(grep -v '^#' .env.testing | xargs); vendor/bin/wp config create --dbname=$TEST_SITE_DB_NAME --dbuser=$TEST_SITE_DB_USER --dbpass=$TEST_SITE_DB_PASSWORD --path=wordpress || true",
            "Kporras07\\ComposerSymlinks\\ScriptHandler::createSymlinks",
            "mkdir -p wordpress/wp-content/uploads",
            "BrianHenryIE\\ComposerPhpStorm\\ExcludeFolders::update",
            "BrianHenryIE\\ComposerPhpStorm\\PHPUnitRunConfigurations::update"
        ],
        "post-update-cmd": [
            "\"vendor/bin/mozart\" compose",
            "vendor/bin/wp core download --path=wordpress || true;",
            "export $(grep -v '^#' .env.testing | xargs); vendor/bin/wp config create --dbname=$TEST_SITE_DB_NAME --dbuser=$TEST_SITE_DB_USER --dbpass=$TEST_SITE_DB_PASSWORD --path=wordpress || true",
            "Kporras07\\ComposerSymlinks\\ScriptHandler::createSymlinks",
            "mkdir -p wordpress/wp-content/uploads",
            "BrianHenryIE\\ComposerPhpStorm\\ExcludeFolders::update",
            "BrianHenryIE\\ComposerPhpStorm\\PHPUnitRunConfigurations::update"
        ]
    }
}
